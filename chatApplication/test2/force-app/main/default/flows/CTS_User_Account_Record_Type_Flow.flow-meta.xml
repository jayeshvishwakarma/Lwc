    <formulas>
        <name>DirectFormula</name>
        <dataType>String</dataType>
        <expression>IF(BEGINS({!vrAcctRecordTypeDeveloperName}, &quot;CTS_EU&quot;), &quot;CTS_EU&quot;,
IF(BEGINS({!vrAcctRecordTypeDeveloperName}, &quot;CTS_MEIA&quot;), &quot;CTS_MEIA&quot;,
IF(BEGINS({!vrAcctRecordTypeDeveloperName}, &quot;CTS_LATAM&quot;), &quot;CTS_LATAM&quot;,
IF(BEGINS({!vrAcctRecordTypeDeveloperName}, &quot;CTS_AP&quot;), &quot;CTS_AP&quot;,
{!vrAcctRecordTypeDeveloperName}))))</expression>
    </formulas>
    <formulas>
        <name>EndUserFormula</name>
        <dataType>String</dataType>
        <expression>IF(BEGINS({!vrAcctRecordTypeDeveloperName}, &quot;CTS_EU&quot;), &quot;CTS_EU_Distributor_End_User&quot;,
IF(BEGINS({!vrAcctRecordTypeDeveloperName}, &quot;CTS_MEIA&quot;), &quot;CTS_MEIA_Distributor_End_User&quot;,
IF(BEGINS({!vrAcctRecordTypeDeveloperName}, &quot;CTS_LATAM&quot;), &quot;CTS_LATAM_Distributor_End_User&quot;,
IF(BEGINS({!vrAcctRecordTypeDeveloperName}, &quot;CTS_AP&quot;), &quot;CTS_AP_Distributor_End_User&quot;,
{!vrAcctRecordTypeDeveloperName}))))</expression>
    </formulas>
    <formulas>
        <name>IndirectFormula</name>
        <dataType>String</dataType>
        <expression>IF(BEGINS({!vrAcctRecordTypeDeveloperName}, &quot;CTS_EU&quot;), &quot;CTS_EU_INDIRECT&quot;,
IF(BEGINS({!vrAcctRecordTypeDeveloperName}, &quot;CTS_MEIA&quot;), &quot;CTS_MEIA_INDIRECT&quot;,
IF(BEGINS({!vrAcctRecordTypeDeveloperName}, &quot;CTS_LATAM&quot;), &quot;CTS_LATAM_INDIRECT&quot;,
IF(BEGINS({!vrAcctRecordTypeDeveloperName}, &quot;CTS_AP&quot;), &quot;CTS_AP_INDIRECT&quot;,
{!vrAcctRecordTypeDeveloperName}))))</expression>
    </formulas>
