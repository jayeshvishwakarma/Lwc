<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignments>
        <name>Set_Fleet_Analysis_Date_to_Current_Date</name>
        <label>Set Fleet Analysis Date to Current Date</label>
        <locationX>408</locationX>
        <locationY>133</locationY>
        <assignmentItems>
            <assignToReference>varAnalysisDateTime</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Flow.CurrentDateTime</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Find_Goal</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Goal_Found</name>
        <label>Goal Found?</label>
        <locationX>409</locationX>
        <locationY>346</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>YESIFOUNDAGOAL</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varFleetAnalysisGoal</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Fleet_Analysis_Goal_on_Fleet_Analysis</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Analysis_Complete_Date_Null</name>
        <label>Is Analysis Complete Date Null?</label>
        <locationX>247</locationX>
        <locationY>134</locationY>
        <defaultConnector>
            <targetReference>Find_Goal</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>YESFLEETANALYSISCOMPLETEDATEISNULL</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varAnalysisDateTime</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Fleet_Analysis_Date_to_Current_Date</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Sales_Rep_Found</name>
        <label>Sales Rep Found?</label>
        <locationX>84</locationX>
        <locationY>128</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varSalesRep</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Is_Analysis_Complete_Date_Null</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <description>Sets the value in teh Fleet Analysis Goal Lookup on Fleet Analysis.</description>
    <formulas>
        <description>The year component of the Fleet Analysis Complete Date.  Used to query Goal records.</description>
        <name>varFleetAnalysisYear</name>
        <dataType>String</dataType>
        <expression>TEXT(YEAR(DATEVALUE({!varAnalysisDateTime})))</expression>
    </formulas>
    <interviewLabel>CC Set Fleet Analysis Goal on Fleet Analysis {!$Flow.CurrentDateTime}</interviewLabel>
    <label>CC Set Fleet Analysis Goal on Fleet Analysis</label>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <name>Find_Goal</name>
        <label>Find Goal</label>
        <locationX>407</locationX>
        <locationY>258</locationY>
        <assignNullValuesIfNoRecordsFound>true</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Goal_Found</targetReference>
        </connector>
        <filters>
            <field>Sales_Rep__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>varSalesRep.Id</elementReference>
            </value>
        </filters>
        <filters>
            <field>Year__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>varFleetAnalysisYear</elementReference>
            </value>
        </filters>
        <object>CC_Fleet_Analysis_Goal__c</object>
        <outputReference>varFleetAnalysisGoal</outputReference>
        <queriedFields>Id</queriedFields>
    </recordLookups>
    <recordLookups>
        <name>Find_Sales_Rep</name>
        <label>Find Sales Rep</label>
        <locationX>84</locationX>
        <locationY>38</locationY>
        <assignNullValuesIfNoRecordsFound>true</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Sales_Rep_Found</targetReference>
        </connector>
        <filters>
            <field>CC_Obsolete__c</field>
            <operator>EqualTo</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <filters>
            <field>CC_Sales_Rep__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>varUserID</elementReference>
            </value>
        </filters>
        <object>CC_Sales_Rep__c</object>
        <outputReference>varSalesRep</outputReference>
        <queriedFields>Id</queriedFields>
    </recordLookups>
    <recordUpdates>
        <name>Set_Fleet_Analysis_Goal_on_Fleet_Analysis</name>
        <label>Set Fleet Analysis Goal on Fleet Analysis</label>
        <locationX>561</locationX>
        <locationY>352</locationY>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>varFleetAnalysisId</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>Fleet_Analysis_Goal__c</field>
            <value>
                <elementReference>varFleetAnalysisGoal.Id</elementReference>
            </value>
        </inputAssignments>
        <object>CC_Fleet_Analysis__c</object>
    </recordUpdates>
    <startElementReference>Find_Sales_Rep</startElementReference>
    <status>Active</status>
