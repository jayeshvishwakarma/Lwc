        <locationX>291</locationX>
        <locationY>481</locationY>
        <assignmentItems>
            <assignToReference>vrCycleTime</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>CycleTimeFormula</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Update_RFQ</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Check_for_file_upload</name>
        <label>Check for file upload</label>
        <locationX>679</locationX>
        <locationY>223</locationY>
        <defaultConnector>
            <targetReference>Loop_through_collection</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Quote NOT Found</defaultConnectorLabel>
        <rules>
            <name>Quote_Found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>vrCollection1</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue/>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>SetFileFoundFlag</targetReference>
            </connector>
            <label>Quote Found</label>
        </rules>
    </decisions>
    <decisions>
        <name>Flag_True</name>
        <label>Flag True</label>
        <locationX>291</locationX>
        <locationY>364</locationY>
        <defaultConnector>
            <targetReference>No_Upload_Alert</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>False</defaultConnectorLabel>
        <rules>
            <name>True</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>FileFound</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Cycle_Time</targetReference>
            </connector>
            <label>True</label>
        </rules>
    </decisions>
    <formulas>
        <name>CompletedDate</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate}</expression>
    </formulas>
    <formulas>
        <name>CurrentUser</name>
        <dataType>String</dataType>
        <expression>{!$User.Id}</expression>
    </formulas>
    <formulas>
        <name>CycleTimeFormula</name>
        <dataType>Number</dataType>
        <expression>IF(({!$Flow.CurrentDate} - {!vrSendFormDate})&lt;1, 0, ({!$Flow.CurrentDate} - {!vrSendFormDate}))</expression>
        <scale>2</scale>
    </formulas>
    <interviewLabel>Buffalo RFQ Complete Quote {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Buffalo RFQ Complete Quote</label>
    <loops>
