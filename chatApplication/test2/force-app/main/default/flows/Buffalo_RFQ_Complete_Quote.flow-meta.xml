    <assignments>
        <name>SetFileFoundFlag</name>
        <label>SetFileFoundFlag</label>
        <locationX>518</locationX>
        <locationY>393</locationY>
        <assignmentItems>
            <assignToReference>FileFound</assignToReference>
            <operator>Assign</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Loop_through_collection</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>Update_Cycle_Time</name>
