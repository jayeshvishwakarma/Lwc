    <decisions>
        <description>If VDU Model is LT 1000 or V3 1001 no modem or sim needed so we skip ahead.</description>
        <name>Skip_Modem_and_SIM</name>
        <label>Skip Modem and SIM?</label>
        <locationX>1191</locationX>
        <locationY>56</locationY>
        <defaultConnector>
            <targetReference>Add_Modem_SN</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>VDU_Model</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>LT_1000</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>VDU_Model</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>V3_1001</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Variables_From_Screen</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
