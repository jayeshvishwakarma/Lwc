        <locationX>657</locationX>
        <locationY>550</locationY>
        <defaultConnector>
            <targetReference>Add_New_VDU</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
