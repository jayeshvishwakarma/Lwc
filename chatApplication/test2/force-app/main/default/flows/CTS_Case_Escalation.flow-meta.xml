        <expression>NOW()</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
BEGINS([Case].OwnerId, &apos;00G&apos;),
NOT(ISBLANK([Case].RecordType.DeveloperName)),
[Case].RecordType.DeveloperName = &apos;NA_Completes_Product_Support&apos;,
[Case].Owner:Queue.DeveloperName = &apos;CTS_NA_Product_Support_Escalated&apos;
)
</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_4</name>
        <dataType>Boolean</dataType>
        <expression>AND(
BEGINS({!myVariable_current.OwnerId}, &apos;00G&apos;),
NOT(ISBLANK({!myVariable_current.RecordType.DeveloperName})),
{!myVariable_current.RecordType.DeveloperName} = &apos;NA_Completes_Product_Support&apos;,
{!myVariable_current.Owner:Queue.DeveloperName} = &apos;CTS_NA_Product_Support_Escalated&apos;
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
[Case].RecordType.DeveloperName = &apos;NA_Completes_Product_Support&apos;,
PRIORVALUE([Case].Status) =  &apos;New&apos;,
OR(
TEXT([Case].Status) = &apos;In Progress&apos;,
TEXT([Case].Status) = &apos;Closed&apos;)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_6</name>
        <dataType>Boolean</dataType>
        <expression>AND(
{!myVariable_current.RecordType.DeveloperName} = &apos;NA_Completes_Product_Support&apos;,
PRIORVALUE({!myVariable_current.Status}) =  &apos;New&apos;,
OR(
TEXT({!myVariable_current.Status}) = &apos;In Progress&apos;,
TEXT({!myVariable_current.Status}) = &apos;Closed&apos;)
)</expression>
    </formulas>
