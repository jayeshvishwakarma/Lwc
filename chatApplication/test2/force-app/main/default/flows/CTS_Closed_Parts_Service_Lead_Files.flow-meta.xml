    <apexPluginCalls>
        <name>Convert_Lead</name>
        <label>Convert Lead</label>
        <locationX>770</locationX>
        <locationY>88</locationY>
        <apexClass>VWFConvertLead</apexClass>
        <connector>
            <targetReference>Assign_New_Opp_Values</targetReference>
        </connector>
        <inputParameters>
            <name>AccountID</name>
            <value>
                <elementReference>sObjLead.Account__c</elementReference>
            </value>
        </inputParameters>
        <inputParameters>
            <name>ConvertedStatus</name>
            <value>
                <stringValue>Converted</stringValue>
            </value>
        </inputParameters>
        <inputParameters>
            <name>CreateOpportunity</name>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </inputParameters>
        <inputParameters>
            <name>LeadID</name>
            <value>
                <elementReference>sObjLead.Id</elementReference>
            </value>
        </inputParameters>
        <inputParameters>
            <name>OpportunityName</name>
            <value>
                <elementReference>frmNewOppName</elementReference>
            </value>
        </inputParameters>
        <outputParameters>
            <assignToReference>sObjNewOpp.Id</assignToReference>
            <name>OpportunityID</name>
        </outputParameters>
    </apexPluginCalls>
    <assignments>
        <name>Assign_New_Opp_Values</name>
        <label>Assign New Opp Values</label>
        <locationX>663</locationX>
        <locationY>308</locationY>
        <assignmentItems>
            <assignToReference>sObjNewOpp.CloseDate</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>$Flow.CurrentDate</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>sObjNewOpp.StageName</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Closed Won</stringValue>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>sObjNewOpp.Amount</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>sObjLead.Parts_Service_Amount__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>sObjNewOpp.CurrencyIsoCode</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>sObjLead.CurrencyIsoCode</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>sObjNewOpp.ForecastCategory</assignToReference>
            <operator>Assign</operator>
            <value>
                <stringValue>Closed</stringValue>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>sObjNewOpp.Parts_Service_Opp__c</assignToReference>
            <operator>Assign</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>sObjNewOpp.Type</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>sObjLead.Parts_or_Service__c</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>sObjNewOpp.Name</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>frmNewOppName</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Update_New_Opp</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Account_on_Lead</name>
        <label>Account on Lead</label>
        <locationX>412</locationX>
        <locationY>81</locationY>
        <defaultConnectorLabel>Account Id NULL</defaultConnectorLabel>
        <rules>
            <name>AccountId_NOT_Null</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>sObjLead.Account__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Convert_Lead</targetReference>
            </connector>
            <label>AccountId NOT Null</label>
        </rules>
    </decisions>
    <formulas>
        <name>frmNewOppName</name>
        <dataType>String</dataType>
        <expression>{!sObjLead.FirstName}&amp;&quot; &quot;&amp;{!sObjLead.LastName}&amp;&quot; &quot;&amp;TEXT({!sObjLead.Parts_or_Service__c}) &amp; &quot; Opportunity&quot;</expression>
    </formulas>
    <interviewLabel>CTS Closed Parts Service Lead Files {!$Flow.CurrentDateTime}</interviewLabel>
    <label>CTS Closed Parts Service Lead Files</label>
    <processMetadataValues>
