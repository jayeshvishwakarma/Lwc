    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND( 
ISNEW(), 
BEGINS([Case].RecordType.DeveloperName, &quot;CTS_TechDirect&quot;),
OR(
        $Profile.Name = &quot;CTS_TechDirect_Supervisor&quot;,
        $Profile.Name = &quot;CTS_TechDirect_Support_Agent&quot;,
        $Profile.Name = &quot;CTS_TechDirect_Regional_Administrator&quot;,
        $Profile.Name = &quot;System Administrator&quot;
))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND( 
ISNEW(), 
BEGINS({!myVariable_current.RecordType.DeveloperName}, &quot;CTS_TechDirect&quot;),
OR(
        {!$Profile.Name} = &quot;CTS_TechDirect_Supervisor&quot;,
        {!$Profile.Name} = &quot;CTS_TechDirect_Support_Agent&quot;,
        {!$Profile.Name} = &quot;CTS_TechDirect_Regional_Administrator&quot;,
        {!$Profile.Name} = &quot;System Administrator&quot;
))</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND (
BEGINS([Case].RecordType.DeveloperName, &quot;CTS_TechDirect&quot;),
OR(BEGINS($Profile.Name, &quot;CTS_TechDirect&quot;), $Profile.Name = &quot;System Administrator&quot;),
ISPICKVAL([Case].Status, &quot;Resolved&quot;) ,
[Case].CTS_TechDirect_Create_Article__c = True
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_5</name>
        <dataType>Boolean</dataType>
