    </formulas>
    <formulas>
        <name>RemainingAssignmentGroups</name>
        <dataType>String</dataType>
        <expression>TRIM(RIGHT( {!vrSelectedAssignmentGroups},({!Length}-20) ))</expression>
    </formulas>
    <formulas>
        <name>Single_AssignmentGroup</name>
        <dataType>String</dataType>
        <expression>LEFT({!vrSelectedAssignmentGroups}, 18)</expression>
    </formulas>
