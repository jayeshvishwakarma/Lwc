    <formulas>
        <name>Counter</name>
        <dataType>Number</dataType>
        <expression>CEILING({!Length}/20)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>Length</name>
        <dataType>Number</dataType>
        <expression>LEN({!vrSelectedAssignmentGroups})</expression>
