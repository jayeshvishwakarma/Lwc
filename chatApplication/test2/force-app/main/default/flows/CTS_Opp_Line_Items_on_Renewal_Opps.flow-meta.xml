    <formulas>
        <name>EUProductCategoryFormula</name>
        <dataType>String</dataType>
        <expression>IF(CONTAINS({!sObjRenewalOpp.Name}, &quot;PackageCARE&quot;), &quot;PackageCARE&quot;,
IF(CONTAINS({!sObjRenewalOpp.Name}, &quot;PlannedCARE&quot;), &quot;PlannedCARE&quot;,
IF(CONTAINS({!sObjRenewalOpp.Name}, &quot;Preventive Maintenance&quot;), &quot;PM Agreement&quot;,
IF(CONTAINS({!sObjRenewalOpp.Name}, &quot;PartsCARE&quot;), &quot;PartsCARE&quot;,
null))))</expression>
    </formulas>
    <formulas>
        <name>EUProductTypeFormula</name>
        <dataType>String</dataType>
        <expression>IF(CONTAINS({!sObjRenewalOpp.Name}, &quot;PackageCARE&quot;), &quot;PackageCARE Renewal&quot;,
IF(CONTAINS({!sObjRenewalOpp.Name}, &quot;PlannedCARE&quot;), &quot;PlannedCARE Renewal&quot;,
IF(CONTAINS({!sObjRenewalOpp.Name}, &quot;Preventive Maintenance&quot;), &quot;PM Agreement Renewal&quot;,
IF(CONTAINS({!sObjRenewalOpp.Name}, &quot;PartsCARE&quot;), &quot;PartsCARE Renewal&quot;,
null))))</expression>
    </formulas>
    <formulas>
        <name>OppLineItemRTFormula</name>
        <dataType>String</dataType>
        <expression>IF((BEGINS({!sObjRenewalOpp.RecordTypeName__c}, &quot;CTS_MEIA&quot;) &amp;&amp; UPPER({!sObjAccount.ShippingCountry}) = &quot;INDIA&quot;), &quot;CTS_India&quot;,
IF((BEGINS({!sObjRenewalOpp.RecordTypeName__c}, &quot;CTS_MEIA&quot;) &amp;&amp; UPPER({!sObjAccount.ShippingCountry}) = &quot;SOUTH AFRICA&quot;), &quot;CTS_South_Africa&quot;,
IF(BEGINS({!sObjRenewalOpp.RecordTypeName__c}, &quot;CTS_EU&quot;), &quot;CTS_EU&quot;,
IF(BEGINS({!sObjRenewalOpp.RecordTypeName__c}, &quot;CTS_LATAM&quot;), &quot;CTS_LATAM&quot;,
IF(BEGINS({!sObjRenewalOpp.RecordTypeName__c}, &quot;CTS_MEIA&quot;), &quot;CTS_ME&quot;,
IF(BEGINS({!sObjRenewalOpp.RecordTypeName__c}, &quot;CTS_AP&quot;), &quot;CTS_AP&quot;,
IF(BEGINS({!sObjRenewalOpp.RecordTypeName__c}, &quot;NA_Air&quot;), &quot;NA_Air&quot;,
null)))))))</expression>
    </formulas>
    <formulas>
        <name>ProductTypeFormula</name>
        <dataType>String</dataType>
        <expression>IF(CONTAINS({!sObjRenewalOpp.Name}, &quot;PackageCARE&quot;), &quot;Service Contracts - PackageCARE&quot;,
IF(CONTAINS({!sObjRenewalOpp.Name}, &quot;PlannedCARE&quot;), &quot;Service Contracts - PlannedCARE&quot;,
IF(CONTAINS({!sObjRenewalOpp.Name}, &quot;Preventive Maintenance&quot;), &quot;Service Contracts - Local PM&quot;,
IF(CONTAINS({!sObjRenewalOpp.Name}, &quot;PartsCARE&quot;), &quot;Service Contracts - PartsCARE&quot;,
IF(CONTAINS({!sObjRenewalOpp.Name}, &quot;Condition&quot;), &quot;Service Contracts - ConditionCARE&quot;,
null)))))</expression>
    </formulas>
