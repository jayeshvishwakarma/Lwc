    $('body').on('DOMNodeInserted', 'div', function () {
        if(document.getElementsByClassName('errorMsg').length>1){            
            document.getElementsByClassName('errorMsg')[0].remove();
        }
        if(document.getElementsByClassName('emptyDependentLookup').length>0){
            document.getElementsByClassName('emptyDependentLookup')[0].className='searchKey glyphicon glyphicon-search form-control';          
