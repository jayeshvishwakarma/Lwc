            <script>
             var prevTextValue = '';
             var newTextValue = '';
            $(document).ready(function(){
                parent.document.getElementsByTagName('iframe')[0].height = $(document).height()+50;
            });
        function dismisSuccess(){
            document.getElementById('successToast').style.display ='none'; 
        }
        function dismisError(){
            document.getElementById('errorToast').style.display ='none';
        }  
        function checkUpdate(){
            var Betalingscondities = document.getElementById('pg:frm:Betalingscondities').value;
            var iban = document.getElementById('pg:frm:iban');
            if(Betalingscondities == 'Automatische incasso' && iban.value == ''){
                iban.style.border = 'solid 1px red';
            }else{
                iban.style.border = 'solid 1px #d8dde6';
                updateData();
            }
            
        }
        function displayUpdateDataCheckboxModel(){
            document.getElementById("modelForCheckBox").style.display = "block";                
        }
        function displayUpdateDataCheckboxModelHide(){
            document.getElementById("modelForCheckBox").style.display = "none";                
        }
        function showModal(obj){
            let modalObj = $(obj).closest("tr").find(".descriptionModal").show();  
            prevTextValue = modalObj.find(".modal-textbox").val();
            //console.log(prevTextValue);
        }
        function closeModal(obj){              
            let modalObj = $(obj).closest("tr").find(".descriptionModal");
            newTextValue = modalObj.find(".modal-textbox").val();
            console.log(newTextValue);
            let value = $(obj).val();
            if(value == 'Annuleren'){
                if(prevTextValue != newTextValue){
                    modalObj.find(".modal-textbox").val(prevTextValue);
                }
            }else{
                updateLabel();
            }
            setTimeout(()=>$(".descriptionModal").hide(),100);
        }
        function showVestiModal(){
            $('#VESTIGINGSADRES-Modal').show();
        }
        function closeModals(obj){  
        if($(obj).val() == 'Opslaan'){
            updateAccountDetail(); 
        }                 
        $('#VESTIGINGSADRES-Modal').hide();
        $('#FACTUURADRES-Modal').hide();
        $('#Primaire-Modal').hide();
        }
        function showFactModal(){
            $('#FACTUURADRES-Modal').show();
        }        
        function showPrimaireModal(){
            $('#Primaire-Modal').show();
        } 
        function changeChildAccount(selAccount){
            window.location.href='apex/accountdetail?accId='+selAccount;
        }      
    </script>
